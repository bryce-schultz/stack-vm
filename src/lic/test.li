fn println(value)
{
    print(value);
    print("\n");
}

fn error(code)
{
    print("error: ");
    if (code == 1)
    {
        print("invalid argument");
    }
    else if (code == 2)
    {
        print("out of memory");
    }
    else if (code == 3)
    {
        print("parse error");
    }
    else
    {
        print("unknown error");
    }
    print("\n");
}

fn two_args(a, b)
{
    print("a: ");
    print(a);
    print("\n");
    print("b: ");
    print(b);
    print("\n");
}

fn add(b, a);

fn add(a, b)
{
    return a * b;
}

fn main()
{
    const invalid_arg = 1;
    const out_of_memory = 2;
    const unknown_error = 3;

    println(5);
    println(10);
    println(10!);

    println(add(2, 3));

    error(invalid_arg);
}